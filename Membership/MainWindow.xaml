<Window x:Class="Membership.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Membership"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="I'm Premium" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="240" Height="73" Click="button_Click"/>
        <Button x:Name="button_Copy" Content="I'm Normal" Margin="0,10,10,0" VerticalAlignment="Top" Height="73" Click="button_Copy_Click" HorizontalAlignment="Right" Width="240"/>
        <Button x:Name="button_Copy1" Content="Play" Margin="167,143,167,0" VerticalAlignment="Top" Height="82" Click="button_Copy1_Click"/>

    </Grid>
</Window>
